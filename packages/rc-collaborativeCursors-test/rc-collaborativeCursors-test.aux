        "onClick": "@destroy(getBots('cursorColors', true));\n\nconfigBot.tags.gridPortal = 'cursorColors';\n\nconst colors = CollaborativeCursors.emitter.tags.cursorColors;\n\nconst COLUMNS = 10;\nconst CELL_SIZE = 1;\n\nlet row = 0;\nlet column = 0;\n\nfor (let color of colors) {\n    create({\n        space: 'tempLocal',\n        form: 'mesh',\n        formSubtype: 'gltf',\n        formAddress: CollaborativeCursors.receiver.tags.cursorModel,\n        color,\n        cursorColors: true,\n        cursorColorsX: CELL_SIZE * column,\n        cursorColorsY: CELL_SIZE * row,\n    });\n\n    column++;\n\n    if (column >= COLUMNS) {\n        column = 0;\n        row++;\n    }\n}\n\n",
